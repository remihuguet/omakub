{
    "[javascript]": {
        "diffEditor.ignoreTrimWhitespace": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnType": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnType": true
    },
    "debug.console.fontSize": 15,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.volumes.label": "VolumeName",
    "editor.fontFamily": "'CaskaydiaMono Nerd Font', 'monospace', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": {
        "comments": false,
        "other": false,
        "strings": false
    },
    "editor.stickyScroll.defaultModel": "indentationModel",
    "editor.stickyScroll.enabled": true,
    "editor.suggestSelection": "first",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.ignoreMissingGitWarning": true,
    "git.openRepositoryInParentFolders": "always",
    "markdown.preview.fontSize": 15,
    "notebook.codeActionsOnSave": {
        "notebook.source.fixAll": "explicit",
        "notebook.source.organizeImports": "explicit"
    },
    "notebook.formatOnSave.enabled": true,
    "redhat.telemetry.enabled": false,
    "scm.inputFontSize": 15,
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.fontFamily": "\"CaskaydiaMono Nerd Font\"",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.stickyScroll.enabled": true,
    "thunder-client.followRedirects": false,
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Palenight Operator",
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.applyToAllProfiles": [
        "editor.formatOnSave"
    ],
    "workbench.startupEditor": "none",
    "workbench.tree.enableStickyScroll": true
}